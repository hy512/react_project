{
  "compilerOptions": {
    "outDir": "./dist/ts",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "preserve",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2015"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@page/*": [
        "src/page/*"
      ],
      "@style/*": [
        "src/style/*"
      ],
    }
  }
}